        "import random\n",
        "import string\n",
        "\n",
        "N = 2000000\n",
        "table = [[random.random() for j in range(4)] for i in range(N)]\n",
        "\n",
        "letters = string.ascii_lowercase[:5]\n",
        "for row in table:\n",
        "    row.append(random.choice(letters))\n",
        "\n",
        "subset = [row for row in table if row[-1] in letters]\n",
        "print(subset)\n",
        "    "
